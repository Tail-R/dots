#define FG #aa0000
#define BG #ffffdd
#define C0 #aa0000
#define C1 #ee8888
#define C2 #ee8888
#define C3 #ffcccc
#define C4 #ffaaaa
#define C5 #ffaaaa
#define C6 #ffaaaa
#define C7 #ffffff

!! colors
*.foreground: FG
*.background: BG

*.color0: C0
*.color8: C0

*.color1: C1
*.color9: C1

*.color2:  C2
*.color10: C2

*.color3:  C3
*.color11: C3

*.color4:  C4
*.color12: C4

*.color5:  C5
*.color13: C5

*.color6:  C6
*.color14: C6

*.color7:  C7
*.color15: C7

!! Xterm
XTerm.vt100.faceName: monospace
XTerm.vt100.faceNameDoublesize: azukifontB
XTerm.vt100.faceSize: 12
XTerm.vt100.scrollBar: true
XTerm.vt100.saveLines: 4096

!! Urxvt
URxvt.font:	xft:azukifontB:size=14:weight=regular
URxvt.geometry: 96x32
URxvt.scrollBar: false
URxvt.scrollBar_right: true
URxvt.cursorUnderline: false
URxvt.cursorBlink: true
URxvt.saveLines: 4096
URxvt.internalBorder: 0
URxvt.urlLauncher: firefox
URxvt.underlineURLs: true

URxvt.keysym.Control-Shift-C: eval:selection_to_clipboard
URxvt.keysym.Control-Shift-V: eval:paste_clipboard

!! Sxiv
Sxiv.font: azukifontB-16
Sxiv.foreground: C5
Sxiv.background: C7
